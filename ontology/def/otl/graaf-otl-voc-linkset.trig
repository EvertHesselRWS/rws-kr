@prefix dct: <http://purl.org/dc/terms/> .
@prefix otl: <https://data.rws.nl/def/otl/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix voc: <https://data.rws.nl/def/lts-voc/> .
@prefix void: <http://rdfs.org/ns/void#> .

otl:graaf-otl-voc-linkset {
    otl:Aanstraalverlichting skos:closeMatch voc:Aanstraalverlichting .

    otl:BedieningsEnBesturingsinstallatie skos:relatedMatch voc:3B-Besturingssysteem,
            voc:3B_Besturingsfunctie,
            voc:Besturing_Bediening_en_Bewaking_3B .

    otl:Bedieningspaneel skos:broadMatch voc:Generiek_bediensysteem_RWS .

    otl:CalamiteitenDoorsteekInstallatie skos:closeMatch voc:Calamiteitendoor-steek .

    otl:CameraBeweegbaar skos:narrowMatch voc:Pan-tilt-zoom_CCTV_cameras .

    otl:ClosedCircuitTelevisionInstallatie skos:closeMatch voc:Closed_Circuit_TeleVision .

    otl:DienstgebouwCiviel skos:relatedMatch voc:Dienstgebouw .

    otl:DynamischeBewegwijzeringEnInformatieSystemen skos:closeMatch voc:Dynamisch_Route_Informatie_Paneel .

    otl:Evacuatieverlichting skos:relatedMatch voc:Contourverlichting .

    otl:Geleidebarrier skos:closeMatch voc:Barrier .

    otl:Hulppostkast skos:closeMatch voc:Hulppostkast .

    otl:LokaleBedieningEnBewaking skos:relatedMatch voc:Lokale_bediening .

    otl:NoodbedieningEnBewaking skos:broadMatch voc:Noodbediening .

    otl:Noodhandbediening skos:broadMatch voc:Handbediening .

    otl:SupervisoryControlAndDataAcquisition skos:closeMatch voc:Supervisory_Control_And_Data_Acquisition .

    otl:Tunnel skos:relatedMatch voc:Tunnel_Object .

    otl:Tunnelbuis skos:closeMatch voc:Tunnelbuis .

    otl:Verkeerscentrale skos:relatedMatch voc:VerkeersCentrale .

    otl:VerplaatsbareVangrailInstallatie skos:closeMatch voc:Verrijdbare_bermbeveiliging .

    otl:Videowall skos:closeMatch voc:Videowand .

    otl:Vluchtdeur skos:closeMatch voc:Vluchtdeur .

    otl:Vluchtdeuren skos:closeMatch voc:Vluchtdeur .

    otl:graaf-otl-voc-linkset a void:Linkset ;
        dct:description "De OTL-VOC linkset legt een koppeling tussen OTL-concepten en LTS-VOC-concepten."@nl ;
        dct:title "OTL-VOC linkset"@nl ;
        void:objectsTarget voc: ;
        void:subjectsTarget otl:graaf-kennismodel ;
        owl:imports otl:graaf-kennismodel .
}

